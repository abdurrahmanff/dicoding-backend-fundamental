{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always",
        "jsx": "always",
        "ts": "always",
        "tsx": "always"
      }
    ],
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off"
  }
}
